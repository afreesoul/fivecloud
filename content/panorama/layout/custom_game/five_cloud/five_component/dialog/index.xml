<root>
    <styles>
        <include src="file://{resources}/layout/custom_game/five_cloud/five_component/dialog/index.css" />
    </styles>
    <Panel class="FiveDialogBox" hittest="false" >
        <Label id="FiveDialogTitle" class="FiveDialogTitle" text="-" />
        <Panel class="FiveDialogContent">
            <Panel class="FiveDialogContentErrorlabelPanel">
                <Label id="FiveDialogContentErrorlabel" class="FiveDialogContentErrorlabel" text="-"/>
            </Panel>
            <TextEntry id="FiveDialogInput" class="FiveDialogInput" />
            <Label id="FiveDialogContentlabel" class="FiveDialogContentlabel" text="-" />
        </Panel>
        <Panel class="FiveDialogButtonRow">
            <Button class="FiveDialogButton" id="ok">
                <Label text="确定" />
            </Button>
            <Button class="FiveDialogButton" id="reset">
                <Label text="重置" />
            </Button>
            <Button class="FiveDialogButton" id="cancel" tabindex="1">
                <Label text="取消" />
            </Button>
        </Panel>
    </Panel>
</root>